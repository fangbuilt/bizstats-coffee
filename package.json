{
  "name": "bizstats-coffee",
  "description": "A web app for analyzing coffee statistics using React, TypeScript, and Tailwind CSS.",
  "author": "Bizstats Team",
  "license": "MIT",
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "statistics",
    "coffee",
    "webapp",
    "analytics",
    "data-visualization",
    "bizstats",
    "business-intelligence",
    "data-analysis"
  ],
  "homepage": "https://bizstats-coffee.vercel.app",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "endOfLine": "lf",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "bin": "cli.ts",
  "dependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@tanstack/react-query": "^5.64.2",
    "@tanstack/react-router": "^1.97.21",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.8",
    "fast-json-stable-stringify": "^2.1.0",
    "lucide-react": "^0.515.0",
    "react": "^19",
    "react-dom": "^19",
    "recharts": "^2.15.1",
    "simple-statistics": "^7.8.8",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.19.0",
    "@tanstack/eslint-plugin-query": "^5.65.0",
    "@tanstack/eslint-plugin-router": "^1.98.0",
    "@tanstack/react-query-devtools": "^5.64.2",
    "@tanstack/router-devtools": "^1.97.21",
    "@tanstack/router-plugin": "^1.97.22",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.5.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.5"
  }
}
